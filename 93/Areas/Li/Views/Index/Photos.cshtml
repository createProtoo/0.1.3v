<!-- jQuery Scrollbar -->
<script src="~/Areas/help/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>

<!-- Moment JS -->
<script src="~/Areas/help/assets/js/plugin/moment/moment.min.js"></script>

<!-- Chart JS -->
<script src="~/Areas/help/assets/js/plugin/chart.js/chart.min.js"></script>

<!-- jQuery Sparkline -->
<script src="~/Areas/help/assets/js/plugin/jquery.sparkline/jquery.sparkline.min.js"></script>

