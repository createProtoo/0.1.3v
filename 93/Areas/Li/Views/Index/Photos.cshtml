
@{
    Layout = null;
    @model IEnumerable<Video2018.ImgModel>
}

<!DOCTYPE html>
 
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Photos</title>
    <script src="~/Areas/help/assets/js/plugin/webfont/webfont.min.js"></script>
    <script src="~/Areas/help/assets/js/core/jquery.3.2.1.min.js"></script>
</head>
<body>
    <div style="margin:0 auto;text-align:center">
        @{ 
            foreach (var item in Model)
            {
              <img width=400 src="~/UploadPZ/@item.imgUrl"   />
            }
        }
    </div>
</body>
</html>
<!--   Core JS Files   -->
<script src="~/Areas/help/assets/js/core/popper.min.js"></script>


<!-- jQuery UI -->
<script src="~/Areas/help/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
