@*商户管理页面*@


@{
    ViewBag.Title = "商家管理";
    Layout = null;
    // Layout = "~/Areas/Help/Views/Shared/_Layout.cshtml";
}

<link href="~/Areas/Help/assets/css/My.css" rel="stylesheet" />

<div class="page-header">
    <h4 id="orderTitle" class="page-title">结算列表</h4>

</div>
<div class="row">
    <div class="col-md-12">
        <div class="card" style="width:105%;margin-left:-3%">
            <div class="card-header">

                <div style="display:inline">
                    <div class="row">
                        <div class="col-md-6">
                            <h1>合计: ￥<span id="totalOutMoney">0</span></h1>

                        </div>

                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table id="shop-datatables" class="display table table-striped table-hover">
                        <thead>
                            <tr>
                                @*<th></th>*@
                                <th>订单号</th>
                                <th>UID</th>
                                <th>名称</th>
                                <th>提现金额</th>

                                <th>提现时间</th>
                                <th>订单状态</th>

                                <th>审核账号</th>
                                <th>凭证</th>
                                <th>操作</th>
                                <th>批准时间</th>

                            </tr>
                        </thead>

                        <tbody>

                            <tr>
                            </tr>


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
<script src="~/Areas/Help/assets/js/mm/shopgivemoney.js"></script>

<!--核心样式-->
<link rel="stylesheet" href="~/Areas/Help/assets/css/modallayer.min.css">

